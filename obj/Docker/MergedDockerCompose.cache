name: dockercompose7904801984608816978
services:
  medpoint_api:
    build:
      context: C:\Users\xushn\OneDrive\Desktop\MedPoint
      dockerfile: src/MedPoint.Api/Dockerfile
    container_name: medpoint.api
    depends_on:
      postgres:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    networks:
      default: null
    ports:
      - mode: ingress
        target: 5105
        published: "2003"
        protocol: tcp
  postgres:
    container_name: medpoint.db
    environment:
      POSTGRES_DB: medpointdb
      POSTGRES_PASSWORD: medpoint
      POSTGRES_USER: medpoint
    expose:
      - "5432"
    image: postgres:17
    networks:
      default: null
    volumes:
      - type: volume
        source: pgdata
        target: /var/lib/postgresql/data
        volume: {}
networks:
  default:
    name: dockercompose7904801984608816978_default
volumes:
  pgdata:
    name: dockercompose7904801984608816978_pgdata
    driver: local